			INDS=c(INDS,sampl);
			}
			
			BOT_RIGTH=p1.individuals[which(p1.individuals.x>=5.0 & p1.individuals.y<5.0)];	
			if (length(BOT_RIGTH)>0){
			sampl=sample(BOT_RIGTH,min(25,length(BOT_RIGTH)));
			INDS=c(INDS,sampl);
			}			
			
			BOT_LEFT=p1.individuals[which(p1.individuals.x<5.0 & p1.individuals.y<5.0)];	
			if (length(BOT_LEFT)>0){
			sampl=sample(BOT_LEFT,min(25,length(BOT_LEFT)));
			INDS=c(INDS,sampl);
			}				
				
				
			// INDS=sample(p1.individuals,100,F); //// sample 100 individuals 
			}

			
		else
			{
			INDS=sample(p1.individuals,size(p1.individuals),F);	
			}

		

		MY_WINDOWS=p1.getValue("WINDOWS_XP"); /////Get the Windows
		LENGTH_OF_WINDOW=p1.getValue("LEN_WINDOW");  ////gap of windows

		mysamples = INDS; ////select random individuals for calcs

		HETERO=subpopHeterozygosity(p1,size(mysamples)); ////calc heterozy
		HETEROSD=subpopHeterozygositystd(p1,size(mysamples));
		NEWBORN=which(p1.individuals.age==0);
		RES=mean(p1.spatialMapValue("map1", p1.individuals[NEWBORN].spatialPosition));
		
		
		writeFile(OUT_NAME,paste(c(asString(HETERO),asString(HETEROSD),asString(GEN),asString(p1.individualCount),asString(RES)),'\t'),append=T); /////write it out in our file
		
		
		deleteFile(paste(c(OUT_NAME,asString(GEN),'.vcf'),'_')); ///// Delete previous vcf,gz,tbi file with same name if exist then output vcf,zip it and tabulate it
		deleteFile(paste(c(OUT_NAME,asString(GEN),'.vcf.gz'),'_'));
		deleteFile(paste(c(OUT_NAME,asString(GEN),'.vcf.tbi'),'_'));
		INDS.genomes.outputVCF(filePath = paste(c(OUT_NAME,asString(GEN),'.vcf'),'_'),simplifyNucleotides = F);
		system(paste(c("bgzip",paste(c(OUT_NAME,asString(GEN),'.vcf'),'_'))," "));	
		system(paste(c("tabix -p vcf",paste(c(OUT_NAME,asString(GEN),'.vcf.gz'),'_'))," "));	
		
		
		///////////////////////////////////////////////////// for all individuals in pop
		
		deleteFile(paste(c(OUT_NAME,asString(GEN),'.all_pos'),'_')); ///// do the same with positions
		ALLPOS=NULL;
		
		for (INDHERE in p1.individuals){ /// costly in time
		ALLPOS=c(ALLPOS,paste(c(asString(INDHERE.index),asString(INDHERE.spatialPosition),asString(p1.spatialMapValue("map1",INDHERE.spatialPosition))),'\t'));	
			 // writeFile(paste(c(OUT_NAME,asString(GEN),'.all_pos'),'_'),paste(c(asString(INDHERE.index),asString(INDHERE.spatialPosition),asString(p1.spatialMapValue("map1",INDHERE.spatialPosition))),'\t'),append=T);  ////  x and y positions
		}
		writeFile(paste(c(OUT_NAME,asString(GEN),'.all_pos'),'_'),paste(ALLPOS,'\n'));
		
		
		//////////////////////////////////////////////////// for individuals sampled for vcf file
		
		
		deleteFile(paste(c(OUT_NAME,asString(GEN),'.sampled_pos'),'_'));
		SAMPLEPOS=NULL;
		
		for (INDHERE in INDS){ ///// not so much
		SAMPLEPOS=c(SAMPLEPOS,paste(c(asString(INDHERE.index),asString(INDHERE.spatialPosition),asString(p1.spatialMapValue("map1",INDHERE.spatialPosition))),'\t'));	
			 // writeFile(paste(c(OUT_NAME,asString(GEN),'.sampled_pos'),'_'),paste(c(asString(INDHERE.index),asString(INDHERE.spatialPosition),asString(p1.spatialMapValue("map1",INDHERE.spatialPosition))),'\t'),append=T);  ////  x and y positions
		}
		writeFile(paste(c(OUT_NAME,asString(GEN),'.sampled_pos'),'_'),paste(SAMPLEPOS,'\n'));

		
	}////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////




print(paste(c('Population Size: ',p1.individualCount),'\t'));
